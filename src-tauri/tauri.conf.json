{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "EVE Miner Analyzer",
  "version": "2.3.1",
  "identifier": "com.evemineranalyzer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "EVE Online Strip Miner Roll Analyzer",
        "width": 900,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ]
  },
  "plugins": {
    "tauri-typegen": {
      "project_path": "./src-tauri",
      "output_path": "../src/generated",
      "validation_library": "none",
      "verbose": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
